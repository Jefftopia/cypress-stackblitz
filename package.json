{
  "name": "cypress-cucumber",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress",
    "test": "cypress run --browser chrome",
    "test.headed": "cypress run --browser chrome --headed",
    "delete:reportFolder": "rm -rf reports/",
    "test:cli": "npm run delete:reportFolder && cypress run",
    "merge:reports": "mochawesome-merge reports/results/*.json > reports/results/cypress-combined-report.json",
    "create:html:report": "npm run merge:reports && marge --reportDir reports/tests reports/results/cypress-combined-report.json"
  },
  "keywords": [],
  "author": "Cyrille",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.6.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-downloadfile": "^1.2.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-grep": "^2.13.1",
    "cypress-real-events": "^1.7.0",
    "cypress-recurse": "^1.20.0",
    "cypress-select-tests": "^1.7.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
